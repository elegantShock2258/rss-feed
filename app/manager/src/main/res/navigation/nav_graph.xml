<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/SourceInputFragment">

    <fragment
        android:id="@+id/SourceInputFragment"
        android:name="com.niilopoutanen.rss_feed.manager.SourceInputFragment"
        android:label="Fragment 1"
        tools:layout="@layout/fragment_source_input">

        <action
            android:id="@+id/action_SourceInputFragment_to_SourceStatusFragment"
            app:destination="@id/SourceStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/SourceStatusFragment"
        android:name="com.niilopoutanen.rss_feed.manager.SourceStatusFragment"
        android:label="Fragment 2"
        tools:layout="@layout/fragment_source_status">

        <action
            android:id="@+id/action_SourceStatusFragment_to_SourceInputFragment"
            app:destination="@id/SourceInputFragment" />
    </fragment>
</navigation>